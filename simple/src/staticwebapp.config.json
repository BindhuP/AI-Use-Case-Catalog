{
  "routes": [
    {
      "route": "/",
      "serve": "/index.html"
    },
    {
      "route": "/usecase-details",
      "serve": "/usecase-details.html"
    },
    {
      "route": "/page3",
      "serve": "/page3.html"
    },
    {
      "route": "/src/:path*",
      "serve": "/:path*",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ],
  "mimeTypes": {
    ".js": "application/javascript",
    ".json": "application/json"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/api/*"]
  }
}
