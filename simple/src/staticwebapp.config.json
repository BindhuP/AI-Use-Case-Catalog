{
  "routes": [
    {
      "route": "/",
      "serve": "/src/index.html"
    },
    {
      "route": "/usecase-details",
      "serve": "/src/usecase-details.html"
    },
    {
      "route": "/page3",
      "serve": "/src/page3.html"
    },
    {
      "route": "/data-store.js",
      "serve": "/src/data-store.js"
    },
    {
      "route": "/background.png",
      "serve": "/src/background.png"
    }
  ],
  "navigationFallback": {
    "rewrite": "/src/index.html",
    "exclude": ["/assets/*", "/api/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/src/index.html"
    }
  }
}
